{
    "name": "MSP-Manager",
    "short_name": "MSP-Manager",
    "description": "A web-based companion app to more easily manage partners in Microsoft PartnerCenter.",
    "start_url": "/",
    "display": "standalone",
    "background_color": "#1b1b22",
    "theme_color": "#11111e",
    "orientation": "any", 
    "display_override": [
        "window-controls-overlay"
    ],
    "icons": [
        {
            "src": "media/icon_16.png",
            "sizes": "16x16",
            "type": "image/png"
        },
        {
            "src": "media/icon_32.png",
            "sizes": "32x32",
            "type": "image/png"
        },
        {
            "src": "media/icon_48.png",
            "sizes": "48x48",
            "type": "image/png"
        },
        {
            "src": "media/icon_128.png",
            "sizes": "128x128",
            "type": "image/png"
        },
        {
            "src": "media/icon_512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "media/icon_512.svg",
            "sizes": "512x512",
            "purpose": "any"
        }
    ],
    "screenshots": [
        {
            "src": "media/screenshot.webp",
            "sizes": "1280x720",
            "type": "image/webp",
            "platform": "wide"
        }
    ],
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "text/csv": [
                    ".mspcsv"
                ]
            }
        }
    ],
    "edge_side_panel": {
        "preferred_width": 500
    }
}
